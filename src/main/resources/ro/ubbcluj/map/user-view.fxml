<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="ro.ubbcluj.map.UserController">
    <TableView fx:id="tableView" minHeight="-Infinity" minWidth="-Infinity"
               prefHeight="381.0" prefWidth="663.0" AnchorPane.bottomAnchor="0.0"
               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
               AnchorPane.topAnchor="0.0">
        <columns>
            <TableColumn fx:id="tableColumnFirstName" prefWidth="75.0" text="First Name"/>
            <TableColumn fx:id="tableColumnLastName" prefWidth="75.0" text="Last Name"/>
        </columns>
        <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
        </columnResizePolicy>
    </TableView>
    <HBox spacing="10.0" alignment="CENTER">
        <Button text="Add" onAction="#onAddButtonClick"/>
        <Button text="Update" onAction="#onUpdateButtonClick"/>
        <Button text="Delete" onAction="#onDeleteButtonClick"/>
        <Button text="Exit" onAction="#handleExit"/>
    </HBox>
</VBox>